<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="money_exchange.list">
      <field name="name">money_exchange list</field>
      <field name="model">money_exchange.money_exchange</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="date_change"/>
          <field name="output_amount"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="money_exchange.action_window">
      <field name="name">money_exchange window</field>
      <field name="res_model">money_exchange.money_exchange</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="money_exchange.action_server">
      <field name="name">money_exchange server</field>
      <field name="model_id" ref="model_money_exchange_money_exchange"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="Money Exchange" id="money_exchange.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="money_exchange.menu_1" parent="money_exchange.menu_root"/>
    <menuitem name="Menu 2" id="money_exchange.menu_2" parent="money_exchange.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="money_exchange.menu_1_list" parent="money_exchange.menu_1"
              action="money_exchange.action_window"/>
    <menuitem name="Server to list" id="money_exchange" parent="money_exchange.menu_2"
              action="money_exchange.action_server"/>

  </data>
</odoo>